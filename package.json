{
  "name": "immutable-state-creator",
  "version": "2.0.0-beta.2",
  "description": "utilities to create immutable state",
  "main": "./index.js",
  "types": "./index.d.ts",
  "scripts": {
    "build": "./node_modules/.bin/tsc --declaration",
    "prepublishOnly": "echo \"Error: Don't run 'npm publish' in root. Use 'npm run release' instead.\" && exit 1",
    "test": "npm run test-only",
    "test-only": "jest --coverage",
    "test:watch": "jest --watch",
    "release": "npm run build && cp package.json ./lib && cd ./lib && npm publish --ignore-scripts"
  },
  "devDependencies": {
    "@types/jest": "^26.0.0",
    "immutable": "^4.0.0-rc.12",
    "jest": "^26.0.1",
    "ts-jest": "^26.1.0",
    "typescript": "3.9"
  },
  "peerDependencies": {
    "immutable": "^3.8.1 || ^4.0.0-rc.1"
  },
  "prettier": {
    "trailingComma": "es5",
    "tabWidth": 2,
    "semi": false,
    "singleQuote": true,
    "printWidth": 120
  },
  "author": "Yulong Ruan <ruanyu1@gmail.com>",
  "license": "MIT",
  "keywords": [
    "typescript"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/ruanyl/create-state.git"
  },
  "homepage": "https://github.com/ruanyl/create-state",
  "bugs": "https://github.com/ruanyl/create-state/issues"
}
